    "webextension-polyfill": "^0.2.1"
  },
  "devDependencies": {
    "angular-messages": "^1.7.5",
    "angular-sanitize": "^1.7.5",
    "ansi-colors": "^3.2.3",
    "cross-env": "^5.2.0",
